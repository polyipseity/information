{
    "version": 5,
    "beforeBuildTasks": [],
    "afterBuildTasks": [],
    "global": {
        "$float-abi-type": "softfp",
        "output-debug-info": "enable",
        "misc-control": "--specs=nosys.specs --specs=nano.specs"
    },
    "c/cpp-compiler": {
        "language-c": "c11",
        "language-cpp": "c++11",
        "optimization": "level-debug",
        "warnings": "all-warnings",
        "one-elf-section-per-function": true,
        "one-elf-section-per-data": true,
        "C_FLAGS": "-DAPP_NAME=RM2023_H",
        "CXX_FLAGS": "-fno-exceptions -fno-rtti -fno-threadsafe-statics -DAPP_NAME=RM2023_H"
    },
    "asm-compiler": {},
    "linker": {
        "output-format": "elf",
        "remove-unused-input-sections": true,
        "LD_FLAGS": "RM2024-RDC-Core/Drivers/DJIMotor.o RM2024-RDC-Core/Drivers/DR16.o",
        "LIB_FLAGS": "-lm"
    }
}