{
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "markdownlint-cli2": "^0.20.0"
  },
  "lint-staged": {
    "**/*.{md,markdown}": [
      "pnpm run markdownlint:fix"
    ]
  },
  "name": "information",
  "packageManager": "pnpm@10.28.1",
  "private": true,
  "scripts": {
    "commitlint": "commitlint --from=origin/main --to=HEAD",
    "markdownlint": "markdownlint-cli2",
    "markdownlint:fix": "markdownlint-cli2 --fix",
    "prepare": "husky"
  },
  "type": "module"
}